{
  "name": "babel-plugin-dotenv-import",
  "version": "2.0.0-beta.2",
  "description": "Load environment variables using import statements.",
  "repository": "git@github.com:tusbar/babel-plugin-dotenv-import.git",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava"
  },
  "keywords": [
    "dotenv",
    "babel-plugin"
  ],
  "dependencies": {
    "dotenv": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "ava": "^0.25.0",
    "codecov": "^3.0.0",
    "nyc": "^12.0.2",
    "xo": "^0.22.0"
  },
  "author": "Bertrand Marron",
  "license": "MIT",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "overrides": [
      {
        "files": "test/fixtures/**/*.js",
        "rules": {
          "import/no-unresolved": [
            "error",
            {
              "ignore": [
                "@env",
                "foo"
              ]
            }
          ],
          "no-unused-vars": "off"
        }
      }
    ]
  }
}
